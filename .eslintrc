{
     "parserOptions": {
          "sourceType": "module",
          "ecmaVersion": 2021
     },
     "rules": {
          "semi": "error",
          "comma-spacing": "error",
          "arrow-spacing": "error",
          "array-bracket-spacing": "error",
          "camelcase": "warn",
          "brace-style": "error",
          "prefer-const": "warn",
          "default-case": "error",
          "func-call-spacing": "error",
          "key-spacing": "error"
     }
}
  